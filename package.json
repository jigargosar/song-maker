{
  "name": "song-maker",
  "version": "1.0.0",
  "description": "A simple song maker app using Elm and Parcel",
  "scripts": {
    "clean": "rm -rf .parcel-cache dist prod elm-stuff",
    "start": "pnpm run clean && parcel --no-cache src/index.html",
    "build": "parcel build src/index.html",
    "clean:build": "pnpm run clean && pnpm run build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.15.0",
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 Firefox versions",
    "last 2 Safari versions",
    "last 2 Edge versions",
    "not dead",
    "not IE > 0"
  ],
  "postcss": {
    "plugins": {
      "@tailwindcss/postcss": {}
    }
  },
  "devDependencies": {
    "@parcel/macros": "^2.16.0",
    "@parcel/transformer-elm": "2.16.0",
    "@tailwindcss/postcss": "^4.1.13",
    "@types/node": "^24.5.2",
    "elm": "0.19.1-6",
    "parcel": "2.16.0",
    "postcss": "^8.5.6",
    "tailwind-scrollbar": "^4.0.2",
    "tailwindcss": "^4.1.13"
  },
  "dependencies": {
    "webaudiofont": "^3.0.4"
  }
}
